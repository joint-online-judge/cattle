{
  "private": true,
  "scripts": {
    "gen:api": "openapi --input ./openapi/openapi.json --output ./src/client",
    "prestart": "npm run gen:api",
    "prestart:staging": "npm run gen:api",
    "start": "cross-env MOCK=none UMI_ENV=dev HOST=127.0.0.1 umi dev",
    "start:staging": "cross-env MOCK=none UMI_ENV=staging HOST=127.0.0.1 umi dev",
    "build": "cross-env UMI_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "lodash-es": "^4.17.21",
    "react-markdown": "^6.0.2",
    "remark-gfm": "^1.0.0",
    "umi": "^3.4.14"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.14",
    "ahooks": "^2.10.2",
    "lint-staged": "^10.0.7",
    "openapi-typescript-codegen": "^0.9.3",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
